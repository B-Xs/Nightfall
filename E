wings[1].Thickness=esp.BoxThickness;self.boxDrawings[1].Visible=esp.BoxEnabled;self.boxDrawings[2].From=p;self.boxDrawings[2].To=p+Vector2.new(0,u)self.boxDrawings[2].Color=esp.BoxColor;self.boxDrawings[2].Thickness=esp.BoxThickness;self.boxDrawings[2].Visible=esp.BoxEnabled;local v=p+Vector2.new(0,o.Y)self.boxDrawings[3].From=v;self.boxDrawings[3].To=v+Vector2.new(u,0)self.boxDrawings[3].Color=esp.BoxColor;self.boxDrawings[3].Thickness=esp.BoxThickness;self.boxDrawings[3].Visible=esp.BoxEnabled;self.boxDrawings[4].From=v;self.boxDrawings[4].To=v+Vector2.new(0,-u)self.boxDrawings[4].Color=esp.BoxColor;self.boxDrawings[4].Thickness=esp.BoxThickness;self.boxDrawings[4].Visible=esp.BoxEnabled;local w=p+Vector2.new(o.X,0)self.boxDrawings[5].From=w;self.boxDrawings[5].To=w+Vector2.new(-u,0)self.boxDrawings[5].Color=esp.BoxColor;self.boxDrawings[5].Thickness=esp.BoxThickness;self.boxDrawings[5].Visible=esp.BoxEnabled;self.boxDrawings[6].From=w;self.boxDrawings[6].To=w+Vector2.new(0,u)self.boxDrawings[6].Color=esp.BoxColor;self.boxDrawings[6].Thickness=esp.BoxThickness;self.boxDrawings[6].Visible=esp.BoxEnabled;local x=w+Vector2.new(0,o.Y)self.boxDrawings[7].From=x;self.boxDrawings[7].To=x+Vector2.new(-u,0)self.boxDrawings[7].Color=esp.BoxColor;self.boxDrawings[7].Thickness=esp.BoxThickness;self.boxDrawings[7].Visible=esp.BoxEnabled;self.boxDrawings[8].From=x;self.boxDrawings[8].To=x+Vector2.new(0,-u)self.boxDrawings[8].Color=esp.BoxColor;self.boxDrawings[8].Thickness=esp.BoxThickness;self.boxDrawings[8].Visible=esp.BoxEnabled end;function i:DrawName(p,o)local y=Vector2.new(p.X+o.X/2,p.Y-15)self.nameDrawing.Position=y;self.nameDrawing.Text=self.player.Name;self.nameDrawing.Color=esp.NameColor;self.nameDrawing.Visible=esp.NameEnabled end;function i:DrawTracer(position,p,o)if esp.TracerEnabled then self.tracer.From=Vector2.new(a.ViewportSize.X/2,a.ViewportSize.Y)self.tracer.To=Vector2.new(p.X+o.X/2,p.Y+o.Y)self.tracer.Color=esp.TracerColor;self.tracer.Thickness=esp.BoxThickness;self.tracer.Visible=esp.TracerEnabled else self.tracer.Visible=false end end;function i:HideBox()for _,z in pairs(self.boxDrawings)do z.Visible=false end;self.nameDrawing.Visible=false;self.tracer.Visible=false end;local function A()if not esp.MasterSwitch then for _,B in pairs(c)do B:HideBox()end;return end;for _,C in ipairs(game.Players:GetPlayers())do if C~=game.Players.LocalPlayer and C.Character and C.Character:FindFirstChild("HumanoidRootPart")then if not c[C]then c[C]=i:New(C)end;c[C]:Update()elseif c[C]then c[C]:HideBox()end end end;game.Players.PlayerAdded:Connect(function(C)C.CharacterAdded:Connect(function(l)if not c[C]then c[C]=i:New(C)end end)end)game.Players.PlayerRemoving:Connect(function(C)if c[C]then c[C]:HideBox()c[C]=nil end end)b.RenderStepped:Connect(A)
